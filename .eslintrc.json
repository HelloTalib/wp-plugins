{
  "root": true,
  "extends": ["react-app", "react-app/jest"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "overrides": [
    {
      "files": ["**/*.js", "**/*.jsx"],
      "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
      }
    }
  ]
}